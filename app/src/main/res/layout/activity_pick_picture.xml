<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_cut"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.PickPictureActivity"
    android:orientation="vertical">

    <include layout="@layout/toolbar_general"></include>


    <FrameLayout
        android:id="@+id/frame_layout"
        android:layout_marginTop="@dimen/activity_horizontal_margin_small"
        android:layout_width="match_parent"
        android:background="@color/gainsboro"
        android:layout_height="200dp">
        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/imageView"
            android:src="@mipmap/ic_pick_pic"
            android:layout_gravity="center" />
    </FrameLayout>






</LinearLayout>
